{
  "name": "vite-sandbox-ssr",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=development tsx server.tsx",
    "start": "NODE_ENV=production tsx server.tsx",
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production vite build --outDir dist/client",
    "build:server": "NODE_ENV=production vite build --outDir dist/server --ssr src/entry-server.js"
  },
  "dependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.29",
    "@types/pretty": "^2.0.3",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "pretty": "^2.0.0",
    "sirv": "^3.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vynn": "workspace:^"
  },
  "devDependencies": {
    "@types/compression": "^1",
    "vite-css-modules": "^1.10.0"
  }
}
